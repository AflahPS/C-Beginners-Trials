#include <stdio.h>
#include <stdlib.h>

int main(void) {


	int num,result;

	setbuf(stdout,NULL);

	printf("Please enter a number");
	scanf("%d",&num);

	int fib(int num1){

		result=fib(num1-1)+fib(num1-2);
		return result;
	}

	printf("%l",result);

		return 0;
}
