#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int sample[50],i,j;

	for (i=0;i<12;i++){
		if(i%2!=0){
			sample[i] = i+1;
		}else{
		sample[i] = i ;
		}
	}
	printf(sample);









	return EXIT_SUCCESS;
}
