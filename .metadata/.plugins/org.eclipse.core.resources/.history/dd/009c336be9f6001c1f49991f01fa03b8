#include <stdio.h>
#include <stdlib.h>

int main(void) {


	int num,result;

	printf("Please enter a number");
	scanf("%d",&num);

	int fib(int num){

		result=num*fib(num-1);

		printf(result+"");

		return 0;
	}/* prints !!!Hello World!!! */
		return 0;
}
