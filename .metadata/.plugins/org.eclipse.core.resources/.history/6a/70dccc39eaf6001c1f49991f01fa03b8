#include <stdio.h>
#include <stdlib.h>

int main(void) {


	int num;

	printf("Please enter a number");
	scanf("%d",&num);

	int fib(int num1){

		return fib(num1-1)+fib(num1-2);
	}

	printf("%d",fib(num));

		return 0;
}
